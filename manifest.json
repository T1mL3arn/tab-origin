{
    "manifest_version": 3,
    "name": "[DEV] Tab Origin",
    "description": "Return to the page from which you opened this tab.",
    "author": "Peter Elmers",
    "version": "1.0.4",
    "icons": {
        "48": "drawing.png",
        "128": "drawing128.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_icon": "drawing.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+U"
            }
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options/options.html",
        "browser_style": false
    }
}
